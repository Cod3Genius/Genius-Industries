# Nhost Environment Variables - GENIUS INDUSTRIES

# Nhost Configuration
NHOST_BACKEND_URL=http://localhost:1337
NHOST_ADMIN_SECRET=your-admin-secret-key
NHOST_JWT_SECRET=your-jwt-secret-key

# Webhook Secrets
WEBHOOK_SECRET=your-webhook-secret-key

# Database Configuration
POSTGRES_DB=genius_industries
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-database-password

# Storage Configuration
STORAGE_BUCKET_PUBLIC=public
STORAGE_BUCKET_PRIVATE=private

# Authentication
AUTH_SMTP_HOST=smtp.example.com
AUTH_SMTP_PORT=587
AUTH_SMTP_USER=your-smtp-user
AUTH_SMTP_PASS=your-smtp-password
AUTH_SMTP_SENDER=noreply@geniusindustries.com

# Security
CORS_ORIGINS=http://localhost:3000,http://localhost:8000
JWT_EXPIRY=3600 